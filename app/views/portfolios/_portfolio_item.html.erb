<div class="card" data-id="<%= portfolio.id %>">
  <%= image_tag portfolio_img(portfolio.thumb_image), width: '100%' unless portfolio.thumb_image.nil? %>
  <p class="card-text">
    <span><%= link_to portfolio.title, portfolio_show_path(portfolio) %></span>
    <%= portfolio.subtitle %>
  </p>
</div>